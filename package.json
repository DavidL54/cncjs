  "jest": {
    "setupFiles": [
      "<rootDir>/jest.env-setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "testTimeout": 10000,
    "testEnvironment": "node"
  },
  "pre-push": [
    "lint"
  ],
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/cncjs"
  },
  "tap": {
    "check-coverage": true,
    "lines": 80,
    "functions": 60,
    "branches": 60,
    "statements": 80
